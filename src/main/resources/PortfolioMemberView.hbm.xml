<?xml version="1.0" encoding="UTF-8"?>



<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
        
<hibernate-mapping package="pojoView">
	<class name="PortfolioMemberView" table="PortfolioMemberView">
 		<id name="portfolioMemberId" column = "portfolioMemberId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		
		<property name="parameter" column="parameter" type="java.lang.Double"/>
		<property name="portfolioId" column="portfolioId" type="java.lang.Integer" />
		<property name="assetId" column="assetId" type="java.lang.Integer"/>
		<property name="accountType" column="accountType" type="java.lang.String"/>
		<property name="assetCode" column="assetCode" type="java.lang.String"/>
		<property name="assetLabel" column="assetLabel" type="java.lang.String"/>
		<property name="value_" column="value_" type="java.lang.Double"/>
			
		<property name="annualReturn" column="annualReturn" type="java.lang.Double"/>
		<property name="returnRate" column="returnRate" type="java.lang.Double"/>
		

		
	</class>
	
	
</hibernate-mapping>