<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pojo">

	<class name="PortfolioMember" table="PortfolioMember">
		<id name="portfolioMemberId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		
		<property name="assetParameter" column="parameter" type="java.lang.Double"/>
		<property name="assetId" column="assetId" type="java.lang.Integer"/>
		<property name="portfolioId" column="portfolioId" type="java.lang.Integer"/>



	</class>
</hibernate-mapping>