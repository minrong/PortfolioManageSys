<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
        
<hibernate-mapping package="pojoView">
	<class name="DepositAccountView" table="depositaccountview">
		<id name="assetId" column = "assetId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		
		<property name="assetCode" column="assetCode" type="java.lang.String"/>
		<property name="assetLabel" column="assetLabel" type="java.lang.String"/>
		<property name="apr" column="apr" type="java.lang.Double"/>

		
	</class>
	
	
</hibernate-mapping>