<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pojo">

	<class name="Portfolio" table="Portfolio" >
		<id name="portfolioId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		
		<property name="portfolioCode" column="portfolioCode" type="java.lang.String"/>
		<property name="ownerId" column="ownerId" type="java.lang.Integer"/>
		<property name="beneficiaryId" column="beneficiaryId" type="java.lang.Integer"/>
		<property name="managerId" column="managerId" type="java.lang.Integer"/>
		
	</class>
</hibernate-mapping>